<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app 
   xmlns="http://java.sun.com/xml/ns/javaee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 
  <display-name>Library</display-name>

  <servlet>
    <servlet-name>library</servlet-name>
    <servlet-class>com.google.appengine.library.Library</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>library</servlet-name>
    <url-pattern>/library</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>setDataServiceName</servlet-name>
    <servlet-class>com.google.appengine.library.SetDataServiceName</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>setDataServiceName</servlet-name>
    <url-pattern>/setDataServiceName</url-pattern>
  </servlet-mapping>
</web-app>


